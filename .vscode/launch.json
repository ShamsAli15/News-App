            "name": "Attach to Chrome",
            "port": 9222,
            "request": "attach",
            "type": "pwa-chrome",
            "webRoot": "${workspaceFolder}"
        },
        {
            "name": "project",
            "cwd": "project",
            "request": "launch",
            "type": "dart"
        },
        {
            "name": "project (profile mode)",
            "cwd": "project",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "flut_icons",
            "cwd": "project\\flut_icons",
