        },
        {
            "name": "flut_icons (profile mode)",
            "cwd": "project\\flut_icons",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "project",
            "cwd": "project\\project",
            "request": "launch",
            "type": "dart"
        },
        {
